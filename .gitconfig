[user]
	name = xcj
	email = canjiexie@gmail.com
[core]
	editor = vim
	quotepath = off
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	dt = difftool
	di = diff
[push]
	default = simple
[pull]
	rebase = true
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[merge]
	tool = vimdiff
	conflictstyle = diff3
[mergetool]
	prompt = false
[rerere]
	enabled = true
[sendpack]
	sideband = false
